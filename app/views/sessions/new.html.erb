<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<% provide(:title,"Log in") %>
<h1>登录</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session,url: login_path) do |f| %>

      <%= f.label :"学号" %>
      <%=f.text_field :stuid,class: 'form-control' %>

      <%= f.label :"密码" %>
      <%=f.password_field :pwd,class: 'form-control' %>

      <%=f.submit "登录",class: "btn btn-primary" %>

    <%end %>

  </div>
</div>